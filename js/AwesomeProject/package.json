        "core-microsoft",
        "hermes",
        "react"
      ]
    },
    "bundle": {
      "treeShake": true,
      "plugins": [
        "@office-iss/metro-plugin-recommended",
        "@rnx-kit/metro-plugin-typescript"
      ]
